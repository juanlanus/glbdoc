      *----------------------------------------------------------------*
       2249-PROCESE-CARCT-00EXT-EXT-09.
      *** 00EXP999IN009 *** CUENTAS CON EXPERIENCIA
           MOVE "  -1" TO 00EXP999IN009(1:4)
                          00EXP999VE009(1:4)
                          00EXP999HP009(1:4)
                          00EXP999RO009(1:4)
           MOVE ZEROS TO PFLAG-009
           IF COD-PORTAF-CARACT = "IN" OR "VE" OR "HP" OR "RO"
            IF REG-ABIERTO-CARACT = 1
             IF MESES-MADUR-CARACT >= 12
               EVALUATE COD-PORTAF-CARACT
                 WHEN "IN"
                     MOVE ZEROS TO 00EXP999IN009
                 WHEN "VE"
                     MOVE ZEROS TO 00EXP999VE009
                 WHEN "HP"
                     MOVE ZEROS TO 00EXP999HP009
                 WHEN "RO"
                     MOVE ZEROS TO 00EXP999RO009
               END-EVALUATE
               PERFORM VARYING J-CARACT FROM 1 BY 1 UNTIL J-CARACT > 49
                 IF V-NOR-CARACT(J-CARACT:1) NUMERIC
                    IF V-NOR-CARACT(J-CARACT:1) >= 0
                       COMPUTE PFLAG-009 = PFLAG-009 + 1
                       IF PFLAG-009 >= 12
                        EVALUATE COD-PORTAF-CARACT
                         WHEN "IN"
                          IF FUNCTION NUMVAL(00EXP999IN009(1:4)) < 0
                             MOVE ZEROS TO 00EXP999IN009
                          END-IF
                          MOVE 1 TO 00EXP999IN009
                         WHEN "VE"
                          IF FUNCTION NUMVAL(00EXP999VE009(1:4)) < 0
                             MOVE ZEROS TO 00EXP999VE009
                          END-IF
                          MOVE 1 TO 00EXP999VE009
                         WHEN "HP"
                          IF FUNCTION NUMVAL(00EXP999HP009(1:4)) < 0
                             MOVE ZEROS TO 00EXP999HP009
                          END-IF
                          MOVE 1 TO 00EXP999HP009
                         WHEN "RO"
                          IF FUNCTION NUMVAL(00EXP999RO009(1:4)) < 0
                             MOVE ZEROS TO 00EXP999RO009
                          END-IF
                          MOVE 1 TO 00EXP999RO009
                        END-EVALUATE
                        MOVE 50 TO J-CARACT
                       END-IF
                    END-IF
                 END-IF
               END-PERFORM
             ELSE
               EVALUATE COD-PORTAF-CARACT
                WHEN "IN"
                 MOVE "  -2" TO 00EXP999IN009(1:4)
                WHEN "VE"
                 MOVE "  -2" TO 00EXP999VE009(1:4)
                WHEN "HP"
                 MOVE "  -2" TO 00EXP999HP009(1:4)
                WHEN "RO"
                 MOVE "  -2" TO 00EXP999RO009(1:4)
